{
  "name": "validate-form-7",
  "author": "Ahsan Bilal",
  "version": "0.2.4",
  "description": "A comprehensive data validation package that helps to minimize the amount of code you will write",
  "main": "build/index.js",
  "keywords": [
    "validate",
    "valiator",
    "data",
    "data validation",
    "form validation",
    "schema",
    "schema validation"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node node_modules/isparta cover --report html --report text node_modules/.bin/_mocha",
    "build": "babel src --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src test"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.10.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-ninja/validate-form-7"
  },
  "bugs": {
    "url": "https://github.com/react-ninja/validate-form-7/issues"
  },
  "homepage": "https://github.com/react-ninja/validate-form-7",
  "license": "MIT"
}
